rv64uv_sc_tests = \
  vfwadd.vf_LMUL1SEW32 \
  vfwadd.vf_LMUL2SEW32 \
  vfwadd.vf_LMUL4SEW32 \
  vfwadd.vv_LMUL1SEW32 \
  vfwadd.vv_LMUL2SEW32 \
  vfwadd.vv_LMUL4SEW32 \
  vfwadd.wf_LMUL1SEW32 \
  vfwadd.wf_LMUL2SEW32 \
  vfwadd.wf_LMUL4SEW32 \
  vfwadd.wv_LMUL1SEW32 \
  vfwadd.wv_LMUL2SEW32 \
  vfwadd.wv_LMUL4SEW32



rv64uv_p_tests = $(addprefix rv64uv-p-, $(rv64uv_sc_tests))

spike_tests = $(rv64uv_p_tests)
